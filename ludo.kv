<Button>:
    size_hint: .2, 0.1
    background_color: (0.06, .36, .4, .675)  
<monk>:
    name:"molester"
    mokila:mokila
    mokilal:mokilal
    mokilali:mokilali
    mokilall:mokilall
    lomku:lomku
    strong:strong
    FloatLayout:
        orientation: "vertical"
        spacing: 10
        space_x: self.size[0]/3
        canvas.before: 
            Color: 
                rgba: (1, 1, 1, 1) 
            Rectangle: 
                source:'E:\ytbh.jpg'
                size: root.width, root.height 
                pos: self.pos 
        MDTextField:    
            id:strong
            icon_left: "email"
            pos_hint:{'center_x':0.5, 'center_y':0.94}
            size_hint:0.8,0.1
            helper_text: "This will disappear when you click off"
            helper_text_mode: "persistent"
            hint_text:'Roll'       
        Button:  
            text: 'back'
            pos_hint :{'center_x':0.15, 'center_y':0.2} 
            background_color: (0.06, .36, .4, .675) 
            on_press: root.goodies()
        Button:
            text: 'Passcode'
            pos_hint :{'center_x':0.15, 'center_y':0.5} 
            background_color: (0.06, .36, .4, .675) 
            on_press: root.sonik()
        Button:
            text: 'attandance'
            pos_hint :{'center_x':0.15, 'center_y':0.65} 
            background_color: (0.06, .36, .4, .675) 
            on_press: root.am()
        MDIconButton:
            icon: "card-search"
            pos_hint :{'center_x':0.9, 'center_y':0.95}
            size_hint:0.2,0.2 
            on_press:root.star()
        Button:
            id:lomku
            text: 'event'
            pos_hint :{'center_x':0.15, 'center_y':0.8} 
            background_color: (0.06, .36, .4, .675) 
            on_press: root.good()
        Button:
            text: 'developer'
            pos_hint :{'center_x':0.15, 'center_y':0.35} 
            background_color: (0.06, .36, .4, .675) 
            on_press: root.click()
        MDTextField:
            id : mokila
            hint_text: 'QNxxxxxxx9SSAAC\nenter api key'
            pos_hint: {'center_x':0.65, 'center_y':0.4} 
            size_hint:0.5,0.07
            mode: "rectangle"
            
        MDTextField:
            id : mokilal
            hint_text: 'PQxxxxxxxxJMNL\nenter secretkey'
            pos_hint: {'center_x':0.65, 'center_y':0.3} 
            size_hint:0.5,0.07
            mode: "rectangle"
            
        MDTextField:
            id : mokilali
            hint_text: 'paste excel.csv \nlink'
            pos_hint: {'center_x':0.65, 'center_y':0.5} 
            size_hint:0.5,0.07
            mode: "rectangle"
            
        MDTextField:
            id : mokilall
            hint_text: 'vhj1@gmail.com\nenter email'
            pos_hint: {'center_x':0.65, 'center_y':0.2} 
            size_hint:0.5,0.07
            mode: "rectangle"
            
